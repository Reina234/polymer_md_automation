<?xml version="1.0"?>
<cg>
  <fmatch>
    <!-- Number of frames for block averaging -->
    <frames_per_block>10</frames_per_block>
    <!-- Constrained least squares? -->
    <constrainedLS>false</constrainedLS>
  </fmatch>

  <non-bonded>
    <name>B2-B2</name> <!-- Polymer-solvent interaction -->
    <type1>B2</type1>
    <type2>B2</type2>
    <fmatch>
      <min>0.45</min>
      <max>2.5</max>
      <step>0.05</step>
      <out_step>0.002</out_step>
    </fmatch>
  </non-bonded>
  <non-bonded>
    <name>B2-B1</name> <!-- Polymer-solvent interaction -->
    <type1>B2</type1>
    <type2>B1</type2>
    <fmatch>
      <min>1.0</min>
      <max>2.5</max>
      <step>0.05</step>
      <out_step>0.002</out_step>
    </fmatch>
  </non-bonded>
  <non-bonded>
    <name>CG-CG</name> <!-- Interaction between solvent CG beads -->
    <type1>CG</type1>
    <type2>CG</type2>
    <fmatch>
      <min>0.25</min>
      <max>2.5</max>
      <step>0.05</step>
      <out_step>0.002</out_step>
    </fmatch>
  </non-bonded>

  <non-bonded>
    <name>B2-CG</name> <!-- Polymer-solvent interaction -->
    <type1>B2</type1>
    <type2>CG</type2>
    <fmatch>
      <min>0.2</min>
      <max>2.5</max>
      <step>0.05</step>
      <out_step>0.002</out_step>
    </fmatch>
  </non-bonded>

  <non-bonded>
    <name>B1-CG</name> <!-- Polymer-solvent interaction -->
    <type1>B1</type1>
    <type2>CG</type2>
    <fmatch>
      <min>0.2</min>
      <max>2.5</max>
      <step>0.05</step>
      <out_step>0.002</out_step>
    </fmatch>
  </non-bonded>
  <non-bonded>
    <name>B1-B1</name> <!-- Polymer-solvent interaction -->
    <type1>B1</type1>
    <type2>B1</type2>
    <fmatch>
      <min>1</min>
      <max>2.5</max>
      <step>0.05</step>
      <out_step>0.002</out_step>
    </fmatch>
  </non-bonded>

  <inverse>
    <gromacs>
      <table_bins>0.001</table_bins>
      <pot_max>1000000</pot_max>
      <table_end>2.5</table_end>
    </gromacs>
  </inverse>
</cg>
